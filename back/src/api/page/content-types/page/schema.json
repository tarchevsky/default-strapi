{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "maxLength": 60,
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text",
      "maxLength": 250,
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true,
      "maxLength": 50
    },
    "Dynamic": {
      "type": "dynamiczone",
      "components": [
        "text.paragraph",
        "text.heading",
        "interactivity.cases-carousel",
        "img.img",
        "layout.grid",
        "text.title",
        "decorative.line",
        "img.icon"
      ]
    }
  }
}
